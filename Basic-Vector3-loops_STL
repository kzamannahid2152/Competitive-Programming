vector<ll>v2;

  for(int i=0;i<10;i++){
    int val;
    cin>>val;
     v2.push_back(val);
  }
cout<<"v2"<<endl;

// INDEXING LOOP
 for(int i=0;i<v2.size();i++)cout<<v2[i]<<" ";cout<<endl;
 
// SHORT ITERATOR
  for(auto &it:v2)cout<<it<<" ";cout<<endl;
  
  // BIG ITERATOR...
  for(auto i=v2.begin();i!=v2.end();i++){
    cout<<(*i)<<" ";
  }cout<<endl;

  cout<<v2.size()<<endl;
